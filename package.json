{
  "name": "fibonacci-test",
  "version": "0.1.0",
  "description": "test project for fibonacci numbers",
  "main": "dist/app/index.js",
  "scripts": {
    "prepare": "husky install",
    "scan:secrets": "ggshield secret scan path -y -r .",
    "scan:secrets:precommit": "ggshield secret scan pre-commit",
    "prettier": "prettier --single-quote --write 'src/**/*.ts'",
    "lint": "eslint . --ext .ts",
    "lint:dockerfile": "docker run --rm -i hadolint/hadolint < Dockerfile",
    "build": "rm -rf dist && tsc",
    "start": "node dist/app/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.ts' --coverageReporters='text-summary'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasyl-chyypesh/fibonacci-test.git"
  },
  "author": "Vasyl Chyypesh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasyl-chyypesh/fibonacci-test/issues"
  },
  "homepage": "https://github.com/vasyl-chyypesh/fibonacci-test#readme",
  "dependencies": {
    "amqplib": "0.10.3",
    "body-parser": "1.20.2",
    "express": "4.18.2",
    "redis": "4.6.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@types/amqplib": "^0.10.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
